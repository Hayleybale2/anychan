{
  "private": true,
  "name": "anychan",
  "type": "module",
  "description": "Modern GUI for imageboards (4chan.org, 8ch.net, 2ch.hk, kohlchan.net)",
  "version": "0.3.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "node --no-warnings=ExperimentalWarning --loader ts-node/esm --experimental-json-modules node_modules/webpack/bin/webpack serve --port 1234 --hot --history-api-fallback --module-strict-export-presence --stats-errors --stats-error-details true --mode development --config webpack/webpack.config.ts",
    "build": "npm-run-all build:clean build:create",
    "build:create": "npm-run-all build:bundle build:copy-readme build:move-index-html build:create-announcement-json build:create-manifest-json build:copy-service-worker",
    "build:clean": "rimraf build",
    "build:bundle": "node --no-warnings=ExperimentalWarning --loader ts-node/esm --experimental-json-modules node_modules/webpack/bin/webpack --mode production --progress --module-strict-export-presence --stats-errors --stats-error-details true --color --config ./webpack/webpack.config.prod.ts",
    "build:demo": "cross-env CONFIG_PATH=./configuration/custom.json npm-run-all build create-404-page copy-demo-website-files",
    "build:copy-readme": "cpy readme.build.txt build --flat --rename=readme.txt",
    "build:move-index-html": "npm-run-all build:copy-index-html build:delete-index-html",
    "build:copy-index-html": "cpy build/assets/index.html build --flat",
    "build:delete-index-html": "rimraf build/assets/index.html",
    "build:create-manifest-json": "node --no-warnings=ExperimentalWarning --loader ts-node/esm scripts/create-manifest-json.js",
    "build:create-announcement-json": "echo {} > build/announcement.json",
    "build:copy-service-worker": "cpy assets/service-worker.js build --flat",
    "build:pack": "cd build && cross-var \"jszip-cli add . > ../anychan-$npm_package_version.zip\"",
    "create-404-page": "cpy build/index.html build --flat --rename=404.html",
    "copy-demo-website-files": "cpy website/** build",
    "test": "node --no-warnings=ExperimentalWarning --loader ts-node/esm node_modules/mocha/bin/_mocha --require ./test/setup.js --reporter spec --colors --bail --exit --recursive \"./{,!(node_modules)/**/}*.test.js\""
  },
  "dependencies": {
    "@catamphetamine/id3js": "^1.0.2",
    "@floating-ui/react": "^0.26.16",
    "@formatjs/intl-pluralrules": "^5.2.14",
    "@popperjs/core": "^2.11.8",
    "@sentry/react": "^8.7.0",
    "@types/react": "^18.3.3",
    "classnames": "^2.5.1",
    "core-js": "^3.37.1",
    "easy-react-form": "^2.2.6",
    "fast-memoize": "^2.5.2",
    "file-saver": "^2.0.5",
    "filesize": "^10.1.2",
    "flexible-json-schema": "^0.12.12",
    "imageboard": "^0.11.4",
    "intl-messageformat": "^10.5.14",
    "javascript-time-ago": "^2.5.10",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.2.0",
    "react-focus-lock": "^2.12.1",
    "react-pages": "^0.8.5",
    "react-redux": "^9.1.2",
    "react-responsive-ui": "^0.15.58",
    "react-sortable-dnd-list": "^1.0.3",
    "react-syntax-highlighter": "^15.5.0",
    "react-time-ago": "^7.3.3",
    "redux": "^5.0.1",
    "regenerator-runtime": "^0.14.1",
