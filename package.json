    "flexible-json-schema": "^0.11.22",
    "imageboard": "^0.5.0",
    "intl-messageformat": "^10.1.3",
    "javascript-time-ago": "^2.5.7",
    "lodash-es": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-focus-lock": "^2.9.1",
    "react-pages": "^0.6.21",
    "react-popper": "^2.3.0",
    "react-redux": "^8.0.2",
    "react-responsive-ui": "^0.15.32",
    "react-sortable-dnd-list": "^1.0.1",
    "react-syntax-highlighter": "^15.5.0",
