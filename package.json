{
  "private": true,
  "name": "captchan",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack serve --hot --module-strict-export-presence --stats-errors --stats-error-details --config webpack/webpack.config.dev.babel.js",
    "build": "npm-run-all build:bundle create-announcement-json",
    "build:bundle": "webpack --mode production --progress --module-strict-export-presence --stats-errors --stats-error-details --color --config ./webpack/webpack.config.prod.babel.js",
    "build:demo": "cross-env CONFIG_PATH=./configuration/custom.json npm-run-all build create-404-page",
    "build:pack": "cd build && cross-var \"jszip-cli add . > ../captchan-$npm_package_version.zip\"",
    "create-404-page": "cpy build/index.html build --rename=404.html",
    "create-announcement-json": "echo {} > build/announcement.json"
  },
  "dependencies": {
