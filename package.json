{
  "private": true,
  "name": "anychan",
  "type": "module",
  "description": "Modern GUI for imageboards (4chan.org, 8ch.net, 2ch.hk, kohlchan.net)",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "node --experimental-json-modules node_modules/webpack/bin/webpack serve --port 1234 --hot --history-api-fallback --module-strict-export-presence --stats-errors --stats-error-details true --mode development --config webpack/webpack.config.babel.js",
    "build": "npm-run-all build:bundle create-announcement-json",
    "build:bundle": "node --experimental-json-modules node_modules/webpack/bin/webpack --mode production --progress --module-strict-export-presence --stats-errors --stats-error-details true --color --config ./webpack/webpack.config.prod.babel.js",
    "build:demo": "cross-env CONFIG_PATH=./configuration/custom.json npm-run-all build create-404-page create-index-page",
    "build:pack": "cd build && cross-var \"jszip-cli add . > ../anychan-$npm_package_version.zip\"",
    "create-404-page": "cpy build/index.html build --rename=404.html",
    "create-index-page": "cpy website/index.html build",
    "create-announcement-json": "echo {} > build/announcement.json",
    "test": "node --experimental-json-modules node_modules/mocha/bin/_mocha --require ./test/setup.js --reporter spec --colors --bail --exit --recursive \"./{,!(node_modules)/**/}*.test.js\""
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "^5.1.2",
    "@popperjs/core": "^2.11.6",
    "@sentry/browser": "^7.11.1",
