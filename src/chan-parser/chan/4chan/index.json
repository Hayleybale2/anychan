{
	"id": "4chan",
	"url": "https://www.4chan.org",
	"defaultAuthorName": "Anonymous",
	"attachmentUrl": "https://i.4cdn.org/{boardId}/{name}{ext}",
	"attachmentThumbnailUrl": "https://i.4cdn.org/{boardId}/{name}s.jpg",
	"fileAttachmentUrl": "https://i.4cdn.org/{boardId}/{originalName}{ext}",
	"commentUrlRegExp": "^\\/(.+?)\\/thread\\/(\\d+)#p(\\d+)$",
	"api": {
		"getThread": "http://a.4cdn.org/{boardId}/thread/{threadId}.json",
		"getThreads": "http://a.4cdn.org/{boardId}/catalog.json",
		"getBoards": "http://a.4cdn.org/boards.json"
	}
}